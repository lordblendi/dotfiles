"*":
  "atom-beautify":
    general:
      _analyticsUserId: "98f6a8de-26b8-4d48-a3d0-b538adb773a8"
  "atom-clock":
    dateFormat: "YYYY. MMM. DD., ddd, HH:mm:ss"
    refreshInterval: 1
    showClockIcon: true
  autosave:
    enabled: true
  core:
    audioBeep: false
    disabledPackages: [
      "file-icon-supplement"
      "bookmarks"
      "deprecation-cop"
      "image-view"
      "keybinding-resolver"
      "welcome"
      "update-package-dependencies"
      "timecop"
      "package-generator"
      "metrics"
      "language-csharp"
      "language-clojure"
      "language-go"
      "language-make"
      "language-mustache"
      "language-objective-c"
      "language-property-list"
      "language-toml"
      "language-c"
      "language-perl"
      "language-php"
      "language-ruby-on-rails"
      "about"
      "autoflow"
      "command-palette"
      "encoding-selector"
      "exception-reporting"
      "styleguide"
      "autocomplete-elixir"
    ]
    excludeVcsIgnoredPaths: false
    followSymlinks: false
    ignoredNames: [
      "node_modules"
      "tmp"
      "bower_components"
      "tests"
      "dist"
      ".git"
    ]
    packagesWithKeymapsDisabled: [
      "preview"
      "image-view"
    ]
    projectHome: "/home/nora/Projects"
    themes: [
      "one-dark-ui"
      "atom-dark-syntax"
    ]
  editor:
    autoIndentOnPaste: false
    backUpBeforeSaving: true
    showIndentGuide: true
    showInvisibles: true
    softWrap: true
    useShadowDOM: false
  "ember-tabs":
    overrideUsePods: true
  "exception-reporting":
    userId: "b5a0e3c3-61c1-0597-cbc7-fb0b18c9e16f"
  "file-icon-supplement":
    treeViewIcons: false
  "file-icons":
    forceShow: true
  "find-and-replace":
    projectSearchResultsPaneSplitDirection: "right"
  "fuzzy-finder":
    ignoredNames: [
      "node_modules"
      "tmp"
      "bower_components"
      "tests"
      "dist"
    ]
  "git-diff": {}
  "markdown-preview": {}
  "package-generator":
    packageSyntax: "coffeescript"
  preview:
    _analyticsUserId: "0a37a152-0139-451a-8fd9-e0acc8dd86ec"
  "ruby-block":
    highlightLineNumber: true
  tabs:
    addNewTabsAtEnd: true
    enableVcsColoring: true
  "tree-view":
    focusOnReveal: false
    hideIgnoredNames: true
    hideVcsIgnoredFiles: true
  welcome:
    showOnStartup: false
  wordcount:
    alwaysOn: true
".coffee.source":
  editor:
    autoIndentOnPaste: false
".console.python.text":
  editor:
    autoIndentOnPaste: false
".html.htmlbars.internal.subexp.text":
  editor:
    softWrap: true
".js.source":
  editor:
    autoIndentOnPaste: false
".python.source":
  editor:
    autoIndentOnPaste: false
".python.text.traceback":
  editor:
    autoIndentOnPaste: false
".sass.source":
  editor:
    autoIndentOnPaste: false
".text.xml":
  editor:
    autoIndentOnPaste: false
